<h1><%= @user.name %></h1>

<div>
  <h2>Posts:</h2>
  <ul>
    <% @posts.each do |p| %>
      <li>
        <h2><%= p.title %></h2>
        <p><%= p.body %></p>
        <%= link_to "See Post", p %>
      </li>
    <% end %>
  </ul>
</div>
<div>
</div>
<div>
  <%= link_to "Create Post", new_post_path %>
  <%= link_to "Feed", posts_path %>
</div>
<% if current_user.id == params[:id]%>
  <%= button_to "Sign Out", destroy_user_session_path, method: :delete %>
<% end %>